## Problem 6: Sum Square Difference

fn triangle(n: Int) -> Int:
    n * (n + 1) / 2

fn pyramid(n: Int) -> Int:
    n * (n + 1) * (2*n + 1) / 6

fn solve(N: Int) -> Int:
    triangle(N)**2 - pyramid(N)

test example:
    expect solve(10) == 2_640

@[manual]
test solution:
    expect solve(100) == 25_164_150
