fn solve2(N: Int) -> Int:
    use sieve.primes

    # TODO: Only need to check for higher powers below sqrt(N) == 4.
    fn prime_powers() -> Iter[Int]:
        for p in primes(N):
            a, b = p, p**2
            while b <= N:
                a, b = b, b * p
            yield a

    prime_powers().product()

test example:
