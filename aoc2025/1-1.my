let data1 = embed_str("data/1-example.txt")
let data2 = embed_str("data/1.txt")
let data = data2

fn main() -> Void:
    # TODO: Can `dial` be of type ZZ[100]?
    dial = 50
    count = 0
    for line in data.lines():
        amount = Int::parse(line.slice(1))
        match line[0]:
            'L' => dial = (dial - amount).mod(100)
            'R' => dial = (dial + amount).mod(100)
            _ => unreachable
        if dial == 0:
            count += 1
    print(count)
